{
  "AllowedHosts": "*",
  "Service": {
    "ServiceId": "machine_job"
  },
  "TranslationEngines": [
    "SmtTransfer",
    "Nmt"
  ],
  "BuildJob": {
    "Runners": {
      "Cpu": "Hangfire",
      "Gpu": "ClearML"
    }
  },
  "SmtTransferEngine": {
    "EnginesDir": "/var/lib/machine/engines"
  },
  "ClearML": {
    "ApiServer": "https://api.sil.hosted.allegro.ai",
    "BuildPollingEnabled": false,
    "Project": "production"
  },
  "Logging": {
    "LogLevel": {
      "System.Net.Http.HttpClient.Default": "Warning"
    }
  }
}